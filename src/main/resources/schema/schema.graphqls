type Query {
  board(uuid: ID!): Board
}

type Mutation {
  createBoard(name: String!): Board!
  joinBoard(uuid: ID!): Board!
  registerPlayer(name: String!): Player!
}

type Board {
  uuid: ID!
  name: String!
  playerUuids: [String]!
}

type Player {
  uuid: ID!
  name: String!
  boardUuid: String
}
